{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "belt",
  "word": "_word",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_atom"
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    },
    "_word": {
      "type": "PATTERN",
      "value": "[^-~!@#$%^&*=+|\\\\:.<>/?\\s\\[\\](){}`'\"@#,;0-9][^-~!@#$%^&*=+|\\\\:.<>/?\\s\\[\\](){}`'\"@#,;]*"
    },
    "_decimal_num_literal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[-+]?"
            }
          },
          {
            "type": "PATTERN",
            "value": "[0-9]+(\\.[_0-9]+)?([eE][-+]?[0-9]+)?"
          }
        ]
      }
    },
    "_bin_num_literal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[-+]?"
            }
          },
          {
            "type": "PATTERN",
            "value": "0b[_01]+"
          }
        ]
      }
    },
    "_oct_num_literal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[-+]?"
            }
          },
          {
            "type": "PATTERN",
            "value": "0o[_0-7]+"
          }
        ]
      }
    },
    "_hex_num_literal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[-+]?"
            }
          },
          {
            "type": "PATTERN",
            "value": "0x[_0-9a-fA-F]+"
          }
        ]
      }
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_decimal_num_literal"
        },
        {
          "type": "SYMBOL",
          "name": "_bin_num_literal"
        },
        {
          "type": "SYMBOL",
          "name": "_oct_num_literal"
        },
        {
          "type": "SYMBOL",
          "name": "_hex_num_literal"
        }
      ]
    },
    "escape_sequence": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\"
            },
            {
              "type": "PATTERN",
              "value": "u[0-9a-fA-F]{4}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\"
            },
            {
              "type": "PATTERN",
              "value": "U[0-9a-fA-F]{8}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\"
            },
            {
              "type": "PATTERN",
              "value": "x[0-9a-fA-F]{2}"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\\\[^uUx]"
        }
      ]
    },
    "_str_dq": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "FIELD",
          "name": "string_content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "escape_sequence"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\"]"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_str_sq": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "FIELD",
          "name": "string_content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "escape_sequence"
                },
                {
                  "type": "PATTERN",
                  "value": "[^']"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "_str_tripledq": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\"\"\""
        },
        {
          "type": "FIELD",
          "name": "string_content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "escape_sequence"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\"]"
                },
                {
                  "type": "PATTERN",
                  "value": "\"{1,2}"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "\"\"\""
        }
      ]
    },
    "_str_triplesq": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'''"
        },
        {
          "type": "FIELD",
          "name": "string_content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "escape_sequence"
                },
                {
                  "type": "PATTERN",
                  "value": "[^']"
                },
                {
                  "type": "PATTERN",
                  "value": "'{1,2}"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "'''"
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_str_tripledq"
        },
        {
          "type": "SYMBOL",
          "name": "_str_triplesq"
        },
        {
          "type": "SYMBOL",
          "name": "_str_dq"
        },
        {
          "type": "SYMBOL",
          "name": "_str_sq"
        }
      ]
    },
    "_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "ident": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_word"
        },
        {
          "type": "PATTERN",
          "value": "[-~!@#$%^&*=+|\\\\:.<>/?]*"
        }
      ]
    },
    "operator": {
      "type": "PATTERN",
      "value": "[-~!@#$%^&*=+|\\\\:.<>/?]+"
    },
    "binding": {
      "type": "SYMBOL",
      "name": "ident"
    },
    "pop_assign": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "binding"
        },
        {
          "type": "STRING",
          "value": "="
        }
      ]
    },
    "assign": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "binding"
        },
        {
          "type": "STRING",
          "value": ":="
        }
      ]
    },
    "marker": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "binding"
        },
        {
          "type": "STRING",
          "value": ":"
        }
      ]
    },
    "package_name": {
      "type": "SYMBOL",
      "name": "_word"
    },
    "package_item": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "package_name"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "ident"
        }
      ]
    },
    "_name_like": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "package_item"
        },
        {
          "type": "SYMBOL",
          "name": "assign"
        },
        {
          "type": "SYMBOL",
          "name": "pop_assign"
        },
        {
          "type": "SYMBOL",
          "name": "marker"
        },
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "SYMBOL",
          "name": "operator"
        }
      ]
    },
    "operation": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_atom"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_atom"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "map": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_atom"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "comma": {
      "type": "PATTERN",
      "value": "[,;]"
    },
    "type_notation": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "`"
        },
        {
          "type": "PATTERN",
          "value": "[^`]*"
        },
        {
          "type": "STRING",
          "value": "`"
        }
      ]
    },
    "_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "operation"
        },
        {
          "type": "SYMBOL",
          "name": "array"
        },
        {
          "type": "SYMBOL",
          "name": "map"
        },
        {
          "type": "SYMBOL",
          "name": "comma"
        },
        {
          "type": "SYMBOL",
          "name": "type_notation"
        },
        {
          "type": "SYMBOL",
          "name": "_literal"
        },
        {
          "type": "SYMBOL",
          "name": "_name_like"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "PATTERN",
      "value": "[\\x00-\\x20]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}